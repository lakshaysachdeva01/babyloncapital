<%- include('header'), {seoDetails} %>



<section data-anim-wrap class="pageHero -type-1">
    <div class="pageHero__bg" data-anim-child="img-right cover-white delay-1">
      <img src="/assets/img/pageHero/1.png" alt="image">
    </div>

    <div class="container">
      <div class="row justify-center">
        <div class="col-auto">
          <div class="pageHero__content text-center" data-split='lines' data-anim-child="split-lines delay-3">
            <h1 class="pageHero__title text-white">Rooms & Suites</h1>
            <p class="pageHero__text text-white">Indulge in luxury in our rooms and suites, featuring stunning views, elegant furnishings, and modern amenities.</p>
          </div>
        </div>
      </div>
    </div>

    <div class="pageHero__filter">
      <div data-anim-child="slide-up delay-7" class="searchForm -type-1 -blur-1 rounded-200 -text-white">
        <div class="searchForm__form">
          <div class="searchFormItem js-select-control js-form-dd js-calendar">
            <div class="searchFormItem__button" data-x-click="calendar">
              <div>
                <span class="js-first-date">Check In - Check Out</span>
                <span class="js-last-date"></span>
              </div>
              <i class="icon-arrow-down ml-10"></i>
            </div>


            <div class="searchFormItemDropdown -calendar" data-x="calendar" data-x-toggle="is-active">
              <div class="searchFormItemDropdown__container">

                <div class="searchMenu-date -searchForm js-form-dd js-calendar-el">
                  <div class="searchMenu-date__field shadow-2" data-x-dd="searchMenu-date" data-x-dd-toggle="-is-active">
                    <div class="rounded-4">
                      <div class="elCalendar js-calendar-el-calendar"></div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>

          <div class="searchFormItem js-select-control js-form-dd">
            <div class="searchFormItem__button" data-x-click="rooms">
              <div class="js-select-control-chosen">Rooms</div>
              <i class="icon-arrow-down ml-10"></i>
            </div>
            <div class="searchFormItemDropdown -counter" data-x="rooms" data-x-toggle="is-active">
              <div class="searchFormItemDropdown__container">
                <div class="searchFormItemDropdown__list">

                  <div class="searchFormItemDropdown__item js-select-control-counter">
                    <div class="fw-500 js-search-title">Rooms</div>
                    <div class="d-flex fw-500 js-search-counter">
                      <div class="cursor-pointer size-40 flex-center js-remove">
                        <i class="custom-icon-minus">
                          <span></span>
                        </i>
                      </div>

                      <div class="size-40 flex-center js-number">0</div>

                      <div class="cursor-pointer size-40 flex-center js-add">
                        <i class="custom-icon-plus">
                          <span></span>
                          <span></span>
                        </i>
                      </div>
                    </div>
                  </div>

                </div>
              </div>
            </div>

          </div>

          <div class="searchFormItem js-select-control js-form-dd">
            <div class="searchFormItem__button" data-x-click="guests">
              <div class="js-select-control-chosen">Guests</div>
              <i class="icon-arrow-down ml-10"></i>
            </div>
            <div class="searchFormItemDropdown -counter" data-x="guests" data-x-toggle="is-active">
              <div class="searchFormItemDropdown__container">
                <div class="searchFormItemDropdown__list">

                  <div class="searchFormItemDropdown__item js-select-control-counter">
                    <div class="fw-500 js-search-title">Adults</div>
                    <div class="d-flex fw-500 js-search-counter">
                      <div class="cursor-pointer size-40 flex-center js-remove">
                        <i class="custom-icon-minus">
                          <span></span>
                        </i>
                      </div>

                      <div class="size-40 flex-center js-number">0</div>

                      <div class="cursor-pointer size-40 flex-center js-add">
                        <i class="custom-icon-plus">
                          <span></span>
                          <span></span>
                        </i>
                      </div>
                    </div>
                  </div>

                  <div class="searchFormItemDropdown__item js-select-control-counter">
                    <div class="fw-500 js-search-title">Children</div>
                    <div class="d-flex fw-500 js-search-counter">
                      <div class="cursor-pointer size-40 flex-center js-remove">
                        <i class="custom-icon-minus">
                          <span></span>
                        </i>
                      </div>

                      <div class="size-40 flex-center js-number">0</div>

                      <div class="cursor-pointer size-40 flex-center js-add">
                        <i class="custom-icon-plus">
                          <span></span>
                          <span></span>
                        </i>
                      </div>
                    </div>
                  </div>

                </div>
              </div>
            </div>

          </div>
        </div>

        <div class="searchForm__button">
          <button class="button -accent-1 bg-accent-2 size-50 rounded-full">Go</button>
        </div>
      </div>
    </div>
  </section>

  <section class="layout-pt-lg layout-pb-lg" style="padding-top: 80px;">
    <div class="container">
      <div class="row justify-center text-center">
        <div class="col-xl-6 col-lg-8 col-md-10" data-split='lines' data-anim="split-lines delay-1">
          <h2 class="text-64 md:text-40 lh-11">
            Forget the world outside
          </h2>
          <p class="mt-40">
            Lose yourself in the captivating comforts of our beautifully designed rooms, tailor-made to fit your definition of Luxury.
          </p>
        </div>
      </div>
    </div>

    <div class="px-60 md:px-15">
        
        <div class="roomCard -type-2 pt-100 sm:pt-50">
         
            <div data-anim-wrap class="roomCardGrid -reverse">
              <div class="">
                <div data-anim-child="slide-up delay-3" class="roomCard__content">
                  <div class="d-flex justify-between items-end">
                    <h3 class="roomCard__title lh-065 text-64 md:text-40"> Executive</h3>
                  </div>
    
                 
    
                  <p class="mt-40 md:mt-30">
                    The 410 sq.ft Executive Rooms, provide elite comfort and amenities. The design and accent of the room emits an exquisite ambiance and gives a focused and diversion-free environment accompanied with serene views from the room window.It is embedded with 2 twin beds, a TV set and a work desk .
                  </p>
    
                  <a href="/executive-room-details" class="button -md -type-2 -accent-1 bg-accent-2 mt-50 md:mt-30">ROOM DETAILS</a>
                </div>
              </div>
    
              <div class="">
                <div data-anim-child="img-right cover-white delay-1">
                  <div class="roomCard__image -no-rounded ratio ratio-3:2">
                    <a href="/executive-room-details">
                    <img src="/assets/img/executive.avif" alt="image" class="img-ratio">
                  </a>
                  </div>
                </div>
              </div>
            </div>
        
          </div>

      <div class="roomCard -type-2 layout-pt-lg">
        <div data-anim-wrap class="roomCardGrid">
          <div class="">
            <div data-anim-child="slide-up delay-3" class="roomCard__content">
              <div class="d-flex justify-between items-end">
                <h3 class="roomCard__title lh-065 text-64 md:text-40">Deluxe</h3>
              </div>

             

              <p class="mt-40 md:mt-30">
                Charmingly appointed, The 450 sq. ft. deluxe room has facilities and amenities that you need to make your stay memorable – plush interiors, king-size bed alongside a smart TV, each of our deluxe rooms is designed as a supremely luxurious unit that caters to all of our guests’ requirements.
              </p>

              <a href="/deluxe-room-details" class="button -md -type-2 -accent-1 bg-accent-2 mt-50 md:mt-30">ROOM DETAILS</a>
            </div>
          </div>

          <div class="">
            <div data-anim-child="img-right cover-white delay-1">
              <div class="roomCard__image -no-rounded ratio ratio-3:2">
                <a href="/deluxe-room-details">
                <img src="/assets/img/deluxe.avif" alt="image" class="img-ratio"></a>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="roomCard -type-2 layout-pt-lg">
        <div data-anim-wrap class="roomCardGrid -reverse">
          <div class="">
            <div data-anim-child="slide-up delay-3" class="roomCard__content">
              <div class="d-flex justify-between items-end">
                <h3 class="roomCard__title lh-065 text-64 md:text-40">Super Deluxe</h3>
              </div>

            

              <p class="mt-40 md:mt-30">
                Our highly spacious 460 sq. ft. super-deluxe rooms are perfect for a long staycation. With extravagant facilities of a multipurpose couch, king-sized bed, and smart TV, you will get to experience the lush life. We also offer a fancy minibar, for a small charge, so that you can simply sit back and unwind.
              </p>

              <a href="super-deluxe-room-details" class="button -md -type-2 -accent-1 bg-accent-2 mt-50 md:mt-30">ROOM DETAIL</a>
            </div>
          </div>

          <div class="">
            <div data-anim-child="img-right cover-white delay-1">
              <div class="roomCard__image -no-rounded ratio ratio-3:2">
                <a href="/super-deluxe-room-details">
                <img src="/assets/img/superdeluxe.avif" alt="image" class="img-ratio"></a>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="roomCard -type-2 layout-pt-lg">
        <div data-anim-wrap class="roomCardGrid">
          <div class="">
            <div data-anim-child="slide-up delay-3" class="roomCard__content">
              <div class="d-flex justify-between items-end">
                <h3 class="roomCard__title lh-065 text-64 md:text-40">Suite</h3>
              </div>

              

              <p class="mt-40 md:mt-30">
                Occupying a whopping Min. 500 sq. ft, experience utmost magnificence of our suites, It is embedded with an array of world class facilities such as including a king-sized bed, smart TV, a minibar for a small additional charge, and a private balcony that offers the most scenic views of the city. The suite also offers a separate cozy living room for evoking that extra sense of lavishness for you and your family.
              </p>

              <a href="/suite-room-details" class="button -md -type-2 -accent-1 bg-accent-2 mt-50 md:mt-30">ROOM DETAILS</a>
            </div>
          </div>

          <div class="">
            <div data-anim-child="img-right cover-white delay-1">
              <div class="roomCard__image -no-rounded ratio ratio-3:2">
                <a href="/suite-room-details">
                <img src="/assets/img/suite.avif" alt="image" class="img-ratio"></a>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="roomCard -type-2 layout-pt-lg">
        <div data-anim-wrap class="roomCardGrid -reverse">
          <div class="">
            <div data-anim-child="slide-up delay-3" class="roomCard__content">
              <div class="d-flex justify-between items-end">
                <h3 class="roomCard__title lh-065 text-64 md:text-40">Executive Suite</h3>
              </div>


              <p class="mt-40 md:mt-30">
                Experience sheer opulence, with our executive suites uniquely designed and comfortably furnished, along with a plush living room, It has a separate seating area to laze around after a hectic day of shopping and a cozy comfortable bed to relax.a smart TV, a minibar (for a small additional charge) and a balcony that offers stunning views of the city. A lavish bathtub and jacuzzi is sure to leave you feeling like royalty.
              </p>

              <a href="/executivesuite-room-details" class="button -md -type-2 -accent-1 bg-accent-2 mt-50 md:mt-30">ROOM DETAILS</a>
            </div>
          </div>

          <div class="">
            <div data-anim-child="img-right cover-white delay-1">
              <div class="roomCard__image -no-rounded ratio ratio-3:2">
                <a href="/executivesuite-room-details">
                <img src="/assets/img/executivesuite.avif" alt="image" class="img-ratio"></a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


<%- include('footer') %>